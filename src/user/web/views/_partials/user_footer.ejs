<script>
  document.querySelectorAll("td").forEach(function(td) {
    if (td.innerText.match(/^\d{13}$/)) {
      if (td.innerText == 0) return 'never';
      var date = new Date(parseInt(td.innerText));
      td.innerText = date.toISOString().replace("T", " ").replace(/\.\d+Z$/, "");
    }
  });
</script>
